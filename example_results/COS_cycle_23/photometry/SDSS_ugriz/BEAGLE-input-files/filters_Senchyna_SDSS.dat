name:SDSS_filter_r     min_rel_err:0.02    flux:colName:r_prime   fluxErr:colName:r_prime_err    fileName:$BEAGLE_GENERAL/filters/sdss_r.dat       
name:SDSS_filter_i     min_rel_err:0.02    flux:colName:i_prime   fluxErr:colName:i_prime_err    fileName:$BEAGLE_GENERAL/filters/sdss_i.dat       
name:SDSS_filter_z     min_rel_err:0.02    flux:colName:z_prime   fluxErr:colName:z_prime_err    fileName:$BEAGLE_GENERAL/filters/sdss_z.dat       
